{
  "name": "@aluxian/terraform",
  "version": "1.13.1",
  "description": "A wrapper for platform-specific Terraform binaries via npm",
  "type": "module",
  "license": "MPL-2.0",
  "keywords": [
    "terraform",
    "hashicorp",
    "infrastructure",
    "automation",
    "executable",
    "wrapper",
    "binary",
    "platform"
  ],
  "bin": "bin/terraform.js",
  "files": [
    "bin",
    "hashicorp.asc"
  ],
  "scripts": {
    "lint": "prettier --check .",
    "fix": "prettier --write .",
    "build": "rm -rf ./artifacts && mkdir ./artifacts && mv \"$(npm pack)\" ./artifacts/package.tgz",
    "test": "./tests/run.sh"
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  },
  "optionalDependencies": {
    "@aluxian/terraform-darwin-arm64": "1.13.1",
    "@aluxian/terraform-darwin-x64": "1.13.1",
    "@aluxian/terraform-freebsd-x64": "1.13.1",
    "@aluxian/terraform-linux-arm": "1.13.1",
    "@aluxian/terraform-linux-arm64": "1.13.1",
    "@aluxian/terraform-linux-x64": "1.13.1",
    "@aluxian/terraform-openbsd-x64": "1.13.1",
    "@aluxian/terraform-solaris-x64": "1.13.1",
    "@aluxian/terraform-win32-arm64": "1.13.1",
    "@aluxian/terraform-win32-x64": "1.13.1"
  }
}
